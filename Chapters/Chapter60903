                            <td class="SuSpenSionXu">
                                <label onclick="itemClick(this,'A','single')" name="e0c38c5f-5017-4c15-9e7e-9fedfa349ecc" for="singlelimite0c38c5f-5017-4c15-9e7e-9fedfa349ecc65" class="SuSpenSion">
                                    <input type="radio" style=" vertical-align:middle;" id="singlelimite0c38c5f-5017-4c15-9e7e-9fedfa349ecc65" name="e0c38c5f-5017-4c15-9e7e-9fedfa349ecc" value="A" />
