    <script>
        function ReturnHandler(title, checkLogon, checkUrl, url) {
            var testOver = document.getElementById("over");
            if (testOver != null && testOver.tested && !testOver.saved) {
                var msg = "是否要结束练习并" + title + "吗？";
                if (!confirm(msg)) return;
            }
            if (!checkLogon || isUserLogon()) {
                if (testOver != null && !testOver.saved) {
                    saveTest(false);
                }
                if (checkUrl) url += "?code=" + $("#code").val() + "&groupName=" + $("#groupName").val() + "&TQuestionPlanID=" + $("#TQuestionPlanID").val() + "&tb_l_PaperQuePlanID=" + $("#tb_l_PaperQuePlanIDtypess").val() + "";
                window.location.href = url;
            }
            else {
                if (checkUrl) url += "?code=" + $("#code").val() + "||||groupName=" + $("#groupName").val() + "||||TQuestionPlanID=" + $("#TQuestionPlanID").val() + "||||tb_l_PaperQuePlanID=" + $("#tb_l_PaperQuePlanIDtypess").val() + "";
                window.location.href = "http://tk.100xuexi.com/Member/loginAndReg.aspx?BackUrl=" + url;
            }
        }
    </script>
