    width: 400px; z-index: 1001; height: 270px; position: absolute; top: 50%; left: 50%;
    margin-left: -200px; margin-top: -125px;">
    <div class="close">
        <span class="thicktitle">反馈给我们</span> <span class="colseleft">
            <input type="button" name="clo" value="" class="closelog" onclick="return ShowNof()" />&nbsp;</span></div>
    <div class="fankkuil">
        <table cellpadding="2" cellspacing="1" border="0" bgcolor="#d5d5d5">
            <tr bgcolor="#ffffff">
                <td class="fan_left">
                    试题编号
                </td>
                <td class="fan_righthao">
                </td>
            </tr>
            <tr bgcolor="#ffffff">
                <td class="fan_left1">
                    反馈内容
                </td>
                <td class="fan_right">
                    <textarea class="fedbackarea" cols="5" id="fedbackareaControls" onblur="Clert()"
                        onfocus="Focer()" style="color: #A7A7A7;" rows="5">请在此输入您要反馈的内容，比如：答案错误、解析不妥等等，我们将尽快处理，谢谢您的支持！</textarea>
                </td>
            </tr>
            <tfoot>
                <tr bgcolor="#ffffff">
                    <td colspan="2" style="text-align: center;">
                        <input type="button" value="确定" class="btnarea" />
                        <input type="hidden" id="FeedBack" value="请在此输入您要反馈的内容，比如：答案错误、解析不妥等等，我们将尽快处理，谢谢您的支持！" />
                    </td>
                </tr>
            </tfoot>
        </table>
    </div>
</div>
<script type="text/javascript">
    function ShowNof() {
        $("#feedback").hide();
       
        $("#fedbackareaControls").val("请在此输入您要反馈的内容，比如：答案错误、解析不妥等等，我们将尽快处理，谢谢您的支持！");
        $("#fedbackareaControls").css("color", "#DBDBDB"); //灰
    }
    function Clert() {//失去焦点
        var Content = $("#fedbackareaControls").val();
        var item = $("#FeedBack").val();
        if (Content == null || Content == "") {
            $("#fedbackareaControls").val(item);
            $("#fedbackareaControls").css("color", "#DBDBDB"); //灰
        }
    }
    function Focer() {//获取焦点
        var item = $("#FeedBack").val();
        var Content = $("#fedbackareaControls").val();
        if (Content == item) {
            $("#fedbackareaControls").val("");
            $("#fedbackareaControls").css("color", "#000000"); //黑
        }
    }
</script>

