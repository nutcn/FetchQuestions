                        <tr>
                            <td class="SuSpenSionXu">
                                <label onclick="itemClick(this,'A','single')" name="03b26162-ec00-4149-9b9d-084fd38d06b2" for="singlelimit03b26162-ec00-4149-9b9d-084fd38d06b265" class="SuSpenSion">
                                    <input type="radio" style=" vertical-align:middle;" id="singlelimit03b26162-ec00-4149-9b9d-084fd38d06b265" name="03b26162-ec00-4149-9b9d-084fd38d06b2" value="A" />
