                            <td class="SuSpenSionXu">
                                <label onclick="itemClick(this,'C','single')" name="2fd021b3-cbb5-45df-a473-c4b8de1cd7ce" for="singlelimit2fd021b3-cbb5-45df-a473-c4b8de1cd7ce67" class="SuSpenSion">
                                    <input type="radio" style=" vertical-align:middle;" id="singlelimit2fd021b3-cbb5-45df-a473-c4b8de1cd7ce67" name="2fd021b3-cbb5-45df-a473-c4b8de1cd7ce" value="C" />
