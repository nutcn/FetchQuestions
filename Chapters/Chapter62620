                        <tr>
                            <td class="SuSpenSionXu">
                                <label onclick="itemClick(this,'A','single')" name="692e019e-1864-487f-963f-8155ed284495" for="singlelimit692e019e-1864-487f-963f-8155ed28449565" class="SuSpenSion">
