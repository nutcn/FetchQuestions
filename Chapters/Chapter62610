                            <td class="SuSpenSionXu">
                                <label onclick="itemClick(this,'C','single')" name="2124efdc-bba6-4512-b9ec-30b78b3bd31b" for="singlelimit2124efdc-bba6-4512-b9ec-30b78b3bd31b67" class="SuSpenSion">
