            <dd>
                <table style="width: 100%">
                    <input type="hidden" name="myAnsw" value="" />
                        <tr>
                            <td class="SuSpenSionXu">
                                <label onclick="itemClick(this,'A','single')" name="0e66d799-f79e-4469-931f-8cc92d0cddd2" for="singlelimit0e66d799-f79e-4469-931f-8cc92d0cddd265" class="SuSpenSion">
                                    <input type="radio" style=" vertical-align:middle;" id="singlelimit0e66d799-f79e-4469-931f-8cc92d0cddd265" name="0e66d799-f79e-4469-931f-8cc92d0cddd2" value="A" />
