<script type="text/javascript">
    function ShowNof() {
        $("#feedback").hide();
       
        $("#fedbackareaControls").val("请在此输入您要反馈的内容，比如：答案错误、解析不妥等等，我们将尽快处理，谢谢您的支持！");
        $("#fedbackareaControls").css("color", "#DBDBDB"); //灰
    }
    function Clert() {//失去焦点
        var Content = $("#fedbackareaControls").val();
        var item = $("#FeedBack").val();
        if (Content == null || Content == "") {
            $("#fedbackareaControls").val(item);
            $("#fedbackareaControls").css("color", "#DBDBDB"); //灰
        }
    }
    function Focer() {//获取焦点
        var item = $("#FeedBack").val();
        var Content = $("#fedbackareaControls").val();
        if (Content == item) {
            $("#fedbackareaControls").val("");
            $("#fedbackareaControls").css("color", "#000000"); //黑
        }
    }
</script>

