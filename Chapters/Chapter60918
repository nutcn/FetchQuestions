                        <tr>
                            <td class="SuSpenSionXu">
                                <label onclick="itemClick(this,'A','single')" name="d96b9b07-a6e0-4ec4-91bd-57edd6e87ec2" for="singlelimitd96b9b07-a6e0-4ec4-91bd-57edd6e87ec265" class="SuSpenSion">
                                    <input type="radio" style=" vertical-align:middle;" id="singlelimitd96b9b07-a6e0-4ec4-91bd-57edd6e87ec265" name="d96b9b07-a6e0-4ec4-91bd-57edd6e87ec2" value="A" />
