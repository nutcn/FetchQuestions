        <script type="text/javascript">
            function ziliao_onclick() {
                document.getElementById("ziliao").style.display = "";
            }
            $(document).ready(function () {
                $("#jqxwindow ").jqxWindow({ height: 360, width: 270, theme: 'summer' });
                $("#jqxwindoww ").jqxWindow({ height: 360, width: 270, theme: 'summer' });
            });

            $(document).ready(function () {
                $("#xuanze").click(function () {
                    $("#biaokuan").val("");
                    $(".FontColor").css("border", "1px solid #cccccc");
                    $("#FontColorSer").toggle(); //显示与隐藏
                });
                $("#yan").click(function () {
                    $("#biaokuan").val("");
                    $(".BackgColor").css("border", "1px solid #cccccc");
                    $("#BackgColorSer").toggle(); //显示与隐藏
                });
                $("#BtnBackgColor").click(function () {//背景颜色层
                    $("#biaokuan").val("");
                    $(".FontColor").css("border", "1px solid #cccccc");
                    $("#BackgColorSer").hide();
                });
                $("#BtnFontColor").click(function () {//字体颜色层
                    $("#biaokuan").val("");
                    $(".BackgColor").css("border", "1px solid #cccccc");
                    $("#FontColorSer").hide();
                });
            })
        </script>
