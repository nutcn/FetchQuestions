        <script type="text/javascript">
            function save() {
                //onclick="return save()"
                //字体
                $(".config").css("font-family", $("select[name='Family']").val());
                //字体大小
                $(".config").css("font-size", document.getElementById("zhiti").value + "px");
                //行间距
                var height = $("input[name='LineSpacing']").val() + "%";
                $(".config table tr td ,dd,dt").css({ "line-height": height });
                $("#Settings").hide();
                var bg = $("#bgColor").val();
                var font = $("#FontColor").val();
                $(".config").css("background", bg); //更改当前背景颜色
                $(".config").css("color", font); //更改当前字体颜色
                //$("#form0").submit();
                var bgcolor = bg; //背景颜色
                var fontcolor = font;  //字体颜色
                var LineSpacing = $("input[name='LineSpacing']").val();  //行间距
                var Family = $("select[name='Family']").val();  //字体
                var FontSize = document.getElementById("zhiti").value;
                $.get("/PracticeCenter/VolumSet/UpdateAjax",
                                    {
                                        BgColor: bgcolor,
                                        Family: Family,
                                        FontColor: fontcolor,
                                        FontSize: FontSize,
                                        LineSpacing: LineSpacing,
                                        rand: Math.random()
                                    },
                                    function (data) {
                                        //                                            if (data == "true")
                                        //                                                $("#deptcity").html(data);
                                        //                                            else
                                    });
                alert("更新成功");

                return true;
            }
            //字体
            function god() {
                $("#sample").children().find(".config1").css("font-family", $("select[name='Family']").val());
            }
            //字体大小
            function zhitidaxiao() {
                $("#sample").children().find(".config1").css("font-size", document.getElementById("zhiti").value + "px");
            }
            //行间距
            function changeline() {
                var height = $("input[name='LineSpacing']").val() + "%";
                $("#sample").children().find(".config1 dd,dt,td").css({ "line-height": height });
            }
            //字体颜色
            $('#xuanze').click(function () {
                $('#jqxwindow').jqxWindow('open');
                $('#jqxwindow').jqxWindow('bringToFront');
            });
            $('#caozuo').click(function () {
                $('#jqxwindow').jqxWindow('close');
            });
            $('#closebg').click(function () {
                $('#jqxwindoww').jqxWindow('close');
            });
            //背景颜色
            $('#yan').click(function () {
                $('#jqxwindoww').jqxWindow('open');
                $('#jqxwindoww').jqxWindow('bringToFront');
            });
            $('#caozuor').click(function () {
                $('#jqxwindoww').jqxWindow('close');
            });

            $("#default").click(function () {
                $.post("/PracticeCenter/PaperAjax/GetDefaultConfig", null, function (data) {
                    $("select[name='Family'] option[value='" + data.Family + "']").attr("selected", "selected");
                    $("select[name='Family']").trigger("change");
                    $("#zhiti").val(data.FontSize);
                    $("#zhiti").trigger("onkeyup");
                    //背景
                    $("div[name='beijing']").css("background", data.BgColor);
                    $("#yan").css("background", data.BgColor);
                    $("input[name='BgColor']").val(data.BgColor);
                    //$(".config").css("background", data.BgColor);
                    $(".config1").css("background", data.BgColor);
                    //字体
                    //$(".config").css("color", data.FontColor);
                    $(".config1").css("color", data.FontColor);
                    $("#xuanze").css("background", data.FontColor);
                    $("#xuanze").css("boder", "0");
                    $("input[name='FontColor']").val(data.FontColor);
                    //$(".config").css("font-size", data.Family + "px");
                    $(".config1").css("font-size", data.Family + "px");
                    //行间距
                    var height = data.LineSpacing;
                    $("input[name='LineSpacing']").val(height) + "%";

                    $("#bgColor").val(data.BgColor.substring(1)); //字体颜色
                    $("#FontColor").val(data.FontColor.substring(1)); //背景颜色

                    //$(".config table tr td,dd,dt").css({ "line-height": height + "%" });
                    $(".config1 table tr td,dd,dt").css({ "line-height": height + "%" });
                });
            });
            $("#cancel").click(function () {
                $("#Settings").hide();
            });
        </script>
    </div>
</div>
