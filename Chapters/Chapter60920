                                    <script type="text/javascript">
                                        $(".FontColor").click(function () {
                                            var FColor = $("#biaokuan").val();
                                            if (FColor != null && FColor != "")
                                                $("#" + $("#biaokuan").val()).css("border", "");
                                            $("#biaokuan").val($(this).attr("id")); //当前值给控件
                                            $(this).css("border", "2px solid #007371");
                                            var yanse = $(this).css("background-color");

                                            $(".config1").css("color", yanse);
                                            $("#xuanze").css("background", yanse);
                                            $("#xuanze").css("boder", "0");
                                            $("input[name='FontColor']").val(yanse);
                                            $("#FontColor").val(yanse);
                                        });
                                    </script>
                                </div>
                            </div>
