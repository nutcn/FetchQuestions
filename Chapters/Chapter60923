<script type="text/javascript">

    $(document).ready(function () {

        $(function () {
            //底部
            $(".footer").css("display", "none");
            Showans();
            //.delay(3000).slideUp("slow").click(function () {alert("1");});
        })
        $("body").css("height", "auto"); //自动
        $(".main").css("height", "auto"); //自动
        $(".right").css("height", "auto"); //自动
        $(".right").css("border-top", "1px solid #007371"); //自动
        $(".right_xia").css("height", "auto");
        var height = $(window).height() - 100;
        $(".RightCon01").css("height", height + "px"); //自动
        var leng = 0;

        var width = $(window).width();
        if (width < 1002) {
            $("#box").css("width", "1002px");
        }
        else {
            $("#box").css("width", "auto");
        }

        $(window).resize(function () {
            //$("span").text(x += 1);
            $("body").css("height", "auto"); //自动
            $(".main").css("height", "auto"); //自动
            $(".right").css("height", "auto"); //自动
            $(".right").css("border-top", "1px solid #007371"); //自动
            $(".right_xia").css("height", "auto");
            var height = $(window).height() - 50;
            $(".RightCon01").css("height", height + "px"); //自动

            var leng = 0;


            var width = $(window).width();
            if (width < 1002) {
                $("#box").css("width", "1002px");
            } else {
                $("#box").css("width", "auto");
            }
        });

    });
</script>
