<script type="text/javascript">
    $(document).ready(function () {
        $("[name='Btn_chick']").click(function () {
            CloseStaticresult();
            var Urser_id = $("#HidUserInfor").val();
            if (Urser_id == "" || Urser_id == undefined) {
                var href = $(this).attr("href");
                if (href == "" || href == undefined) {
                    var href = document.URL;
                }
                $("#url").val(href);
                $(this).attr("href", "#");
                //调用蒙层
                var range = getRange();
                $("#doing").css({ width: range.width, height: range.height, display: "block" });
                document.getElementById("login").style.display = "";

                return false;
            }
            else {//成功
                var ict = $(this).attr("Rimd");
                window.location.href = ict + "?tb_l_PaperQuePlanID=" + $("#tb_l_PaperQuePlanIDtypesss").val();
            }
        });
    });
</script>

