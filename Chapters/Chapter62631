                                    <script type="text/javascript">
                                        $(".BackgColor").click(function () {
                                            var FColor = $("#biaokuan").val();
                                            if (FColor != null && FColor != "")
                                                $("#" + $("#biaokuan").val()).css("border", "");
                                            $("#biaokuan").val($(this).attr("id")); //给当前值给控件
                                            $(this).css("border", "2px solid #007371");
                                            var beijingys = $(this).css("background-color");
                                            $("div[name='beijing']").css("background", beijingys);
                                            $("#yan").css("background", beijingys);
                                            $("input[name='BgColor']").val(beijingys);
                                            $(".config1").css("background", beijingys);
                                            $("#bgColor").val(beijingys);

                                        });
                                    </script>
