    <script type="text/javascript">
        function nocopy() {
            var allowHtmlCopy = parseInt("0");
            if (allowHtmlCopy > 0) return;
            document.onselectstart = function () { return false; };
            document.onselect = function () { document.selection.empty(); };
            document.onbeforecopy = function () { return false; };
            document.oncopy = function () { document.selection.empty(); };
            //document.oncontextmenu = function () { return false; };
        }

        function CloseGuideBox() {
            $(".Mask").css("display", "none");
            $(".GuideBox").css("display", "none");
        }
        /*选项卡控制*/
        function setTab(name, cursel, n) {
            for (i = 1; i <= n; i++) {
                var menu = document.getElementById(name + i);
                var con = document.getElementById("con" + name + i);
                menu.className = i == cursel ? "selected" : "";
                con.style.display = i == cursel ? "block" : "none";
            }
        }

        $(function () {
            nocopy();

            DragPubic(document.getElementById("ColerExam")); //页面设置
            $("#ColerExam").css("cursor", "move");
            //LogPubic(document.getElementById("login_Close")); //页面设置
            //$("#login_Close").css("cursor", "move");
            calPubic(document.getElementById("ClacTitle")); //页面设置
            //左侧显示隐藏
            $("#lefter").fadeIn(300, function () {

            });
            setTimeout(leftHide, 3000);
            //底部显示隐藏
            $("#bottom").fadeIn(300, function () {

            });
            setTimeout(bottomHide, 3000);
            //右侧显示隐藏
            $("#righter").fadeIn(300, function () {

            });
            setTimeout(rightHide, 3000);
            //头部显示隐藏
            $("#top").fadeIn(300, function () {

            });
            setTimeout(topHide, 3000);

            $(".BtnCalculator").click(function () {
                $("#calc").show();
            });

            $(".BtnSearch").click(function () {
                $("#search").show();
            });

            //设置当前Left最高高度
            var Heid = $(window).height(); //当前显示器高度
            if ($(".inner").height() >= (Heid - 200)) {//滚动
                $(".zj_nav").css("height", (Heid - 200) + "px");

            }
            else {
                $(".zj_nav").css("height", "auto");
            }
        })

        function Chike(itemID, o) {
            var classt = $(o).find("span").attr("class"); //.addClass("Hid");
            if (classt == "ulShow")
                $(o).find("span").attr("class", "ulHid");
            else
                $(o).find("span").attr("class", "ulShow");
            if ($("#" + itemID).css("display") == "none") {
                $("#" + itemID).css("display", "block");
            }
            else {
                $("#" + itemID).css("display", "none");
            }
            //设置当前Left最高高度
            var Heid = $(window).height(); //当前显示器高度
            if ($(".inner").height() >= (Heid - 200)) {//滚动
                $(".zj_nav").css("height", (Heid - 200) + "px");
            }
            else {
                $(".zj_nav").css("height", "auto");
            }
        }

        function Btn_chick(url) {
            var ict = url;
            //判断用户是否登陆
            $.get("/LoginManager/GetUserName", { rand: Math.random() }, function (data) {
                var strs = new Array(); //定义一数组
                strs = data.split("&&");
                if ($.trim(strs[0]) != "0") {
                    window.location.href = ict + "?code=" + $("#code").val() + "&groupName=" + $("#groupName").val() + "&TQuestionPlanID=" + $("#TQuestionPlanID").val() + "&tb_l_PaperQuePlanID=" + $("#tb_l_PaperQuePlanIDtypess").val() + "";
                }
                else {
                    if ($("#code").val() != "") {
                        exeBuy();
                    }
                    else {
                        window.location.href = "http://tk.100xuexi.com/Member/loginAndReg.aspx?BackUrl=" + ict + "?code=" + $("#code").val() + "||||groupName=" + $("#groupName").val() + "||||TQuestionPlanID=" + $("#TQuestionPlanID").val() + "||||tb_l_PaperQuePlanID=" + $("#tb_l_PaperQuePlanIDtypess").val() + "";
                    }
                }
            });
        }
    </script>
