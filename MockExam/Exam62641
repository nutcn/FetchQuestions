    <script type="text/javascript">



        var isPlay = 0;

        //3d播放
        function onClick3dButton(seconds) {

            videoPlayer.pauseVideo();
            playerVideo($("#videoUrl").val(), 'http://admintk.100xuexi.com/upload/BigPic/201601211139153193.jpg', $("#videotitle").val(), seconds);

        }
        function setVideoInfo(splashUrl, title)//设置视频的封面地址和标题   (播放时调用)
        {
            tkBook.setVideoInfo(splashUrl, title);
        }
        function setVideoSource(url, seconds) {
            tkBook.setVideoSource(url, sencond);
        }
        function setVideoSize(width, height) {
            tkBook.setVideoSize("800", "600");
        }

        function playerVideo(videoUrl, splashUrl, title, seconds) {

            if (isPlay == 1) {
                $("#flex").removeClass("flexclose");
                $("#flex").addClass("flexopen");
                $("#windown-box").css("visibility", 'hidden');
                try {
                    tkBook.setVideoInfo(splashUrl, title);
                    tkBook.setVideoSource(videoUrl, seconds);
                } catch (e) {
                    alert(e);
                }
            }
            else {
                alert("正在加载数据......");
            }
        }

        function setPageInfo(firstUrl, lastUrl) {
            tkBook.setPageInfo(firstUrl, lastUrl);
        }
        function onClose3dVideo(seconds) {





            $("#flex").removeClass("flexopen");
            $("#flex").addClass("flexclose");
            $("#windown-box").css("visibility", 'visible');
            gotoFrame1(seconds);
        }


        function onInitReady() {
            setPageInfo("http://admintk.100xuexi.com/upload/BigPic/201601211139153193.jpg", "http://admintk.100xuexi.com/upload/BigPic/201601211139153193.jpg");
        }

        function onBookDbClick() {
            $("#flex").removeClass("flexopen");
            $("#flex").addClass("flexclose");
            $("#windown-box").css("visibility", 'visible');
            gotoFrame1(seconds);
        }
        //是否加载
        function onPageReady() {
            isPlay = 1;
        }
        var swfUrl = "/tikud/tkBook3D.swf";
        var urlParam = getRequestParam("isInContainer");
        if (urlParam == "true")
            swfUrl = "/tikud/tkBook3D.swf?isInContainer=true";
        var swfVersionStr = "11.1.0";
        var xiSwfUrlStr = "/tikud/playerProductInstall.swf";
        var flashvars = {};
        var params = {};
        params.quality = "high";
        params.bgcolor = "#ffffff";
        params.allowscriptaccess = "*";
        params.allowfullscreen = "true";
        params.wmode = "transparent";
        var attributes = {};
        attributes.id = "tkBook";
        attributes.name = "tkBook";
        attributes.align = "middle";
        swfobject.embedSWF(swfUrl
            , "flashContent",
                "100%", "100%",
                swfVersionStr, xiSwfUrlStr,
                flashvars, params, attributes);
        // JavaScript enabled so display the flashContent div in case it is not replaced with a swf object.
        swfobject.createCSS("#flashContent", "display:block;width:100%;height:100%;");
    </script>
