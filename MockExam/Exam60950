    $(function () {
        $(".WebChat_Rate").click(function () {
            var userID = "10009";
            var platID = 3;
            var productID = $("#TQuestionPlanID").val();
            if (isUserLogon()) {
                userID = $.cookie('xx_UserID');
            }
            WebIM.Home(userID, productID, platID)
        });
    });
</script>

