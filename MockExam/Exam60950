<script type="text/javascript">
    $(function () {
        //底部
        $(".footer").css("display", "none");
        Showans();
        // $('#header').delay(3000).slideUp(); //"slow"
    })

    //$("body").css("height", "auto"); //自动
    $(".main").css("height", "auto"); //自动
    $(".right").css("height", "auto"); //自动
    $(".right").css("border-top", "1px solid #007371"); //自动
    $(".right_xia").css("height", "auto");
    var height = $(window).height() - 100;
    $(".RightCon01").css("height", height + "px"); //自动
    var leng = 0;


    var width = $(window).width();
    if (width < 1002) {
        $("#box").css("width", "1002px");
    }
    else {
        $("#box").css("width", "auto");
    }


</script>
<script type="text/javascript">
    function resizeWindowContent() {
        //$("span").text(x += 1);
        //$("body").css("height", "auto"); //自动
        $(".main").css("height", "auto"); //自动
        $(".right").css("height", "auto"); //自动
        $(".right").css("border-top", "1px solid #007371"); //自动
        $(".right_xia").css("height", "auto");
        var height = $(window).height() - 100;
        $(".RightCon01").css("height", height + "px"); //自动
        //leng = $(".zj_nav").css("height"); //左内框
        //leng = height;  //parseInt(leng) + 130;
        //$(".zj_nav").css("height", leng + "px");
        var width = $(window).width();
        if (width < 1002) {
            $("#box").css("width", "1002px");
        } else {
            $("#box").css("width", "auto");
        }
    }
    $(document).ready(function () {
        resizeWindowContent();
        $(window).resize(resizeWindowContent);
    });
</script>
